KMS_BASE_URL=http://localhost:3000
# Generate a base64url style token (22+ chars) and reuse it across restarts.
KMS_CLIENT_TOKEN=replace-with-your-token

# Where encrypted blobs, the SQLite DB, and sessions are stored.
STORAGE_DATA_DIR=./server/.data

# Optional tuning: chunk size (bytes) per encrypted block.
# STORAGE_CHUNK_SIZE=10485760

# Session lifetime in days.
STORAGE_SESSION_TTL_DAYS=14

# Deterministic verification code secrets
JWT_SECRET=replace-with-long-secret
CAT_NAME=replace-with-cat-name

# Optional: Cloudflare Tunnel token when using docker-compose.cloudflare.yml
CLOUDFLARE_TUNNEL_TOKEN=
